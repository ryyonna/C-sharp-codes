using System;
namespace first
{
    class A
    {
        public static void Main(string[] args)
        {
            Console.WriteLine("Enter a number:");
            int x=Convert.ToInt32(Console.ReadLine());
            int fact = 1;
            for(int i=1;i<=x;i++)
            {
                fact = fact * i;
            }
            Console.WriteLine("Factorial of {0} is: {1}", x, fact);
            Console.ReadKey();
        }
    }
}
